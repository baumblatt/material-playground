<?xml version='1.0' encoding='utf-8'?>
<widget id="com.baumblatt.material.ma01" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Material</name>
    <description>
        An Angular Material and Cordova playground.
    </description>
    <author email="baumblatt@gmail.com" href="https://baumblatt.mybluemix.net">
        Bernardo Baumblatt
    </author>
    <content originalcor-src="index.html" src="http://192.168.43.79:4200" />
    <allow-navigation href="http://192.168.43.79:4200" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
