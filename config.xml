<?xml version='1.0' encoding='utf-8'?>
<widget id="com.baumblatt.material.ma01" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Material</name>
    <description>
        An Angular Material and Cordova playground.
    </description>
    <author email="baumblatt@gmail.com" href="https://baumblatt.mybluemix.net">
        Bernardo Baumblatt
    </author>
    <content originalcor-src="index.html" src="http://192.168.25.3:4200" />
    <allow-navigation href="http://192.168.25.3:4200" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="res/icon/android/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="res/icon/android/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="res/icon/android/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="res/icon/android/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="res/icon/android/drawable-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="res/splash/android/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="res/splash/android/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="res/splash/android/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="res/splash/android/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="res/splash/android/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="res/splash/android/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="res/splash/android/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="res/splash/android/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="res/splash/android/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="res/splash/android/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="res/splash/android/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="res/splash/android/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device" spec="^1.1.7" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.1.0" />
</widget>
